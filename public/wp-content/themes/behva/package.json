{
  "name": "behva",
  "version": "1.0.0",
  "description": "[![Build Status](https://travis-ci.org/Automattic/_s.svg?branch=master)](https://travis-ci.org/Automattic/_s)",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css": "sass --style=compressed assets/scss/import.scss style-theme.css",
    "watch:css": "sass --style=compressed --watch assets/scss/import.scss:style-theme.css",
    "bs": "browser-sync start --proxy 'http://behva.local' --files 'style-theme.css, ../../../wp-content/**/*.css, ../../../wp-content/**/*.js, ../../../wp-content/**/*.php, ../../../wp-content/plugins/**/*.css'",
    "dev": "npm-run-all --parallel watch:css bs"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "global": "^4.4.0",
    "gulp-cli": "^2.3.0",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "browser-sync": "^2.29.3",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^6.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-deporder": "^1.2.0",
    "gulp-imagemin": "^8.0.0",
    "gulp-newer": "^1.4.0",
    "gulp-noop": "^1.0.1",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.1.0",
    "gulp-strip-debug": "^5.0.0",
    "gulp-uglify": "^3.0.2",
    "nodemon": "^2.0.22",
    "postcss-assets": "^6.0.0",
    "sass": "^1.82.0"
  }
}